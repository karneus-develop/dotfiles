{
  "suggest.noselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "languageserver": {
      "dockerfile": {
        "command": "docker-languageserver",
        "filetypes": ["dockerfile"],
        "args": ["--stdio"]
      },
      "python": {
        "command": "python",
        "filetypes": ["python"]
      }
  },
  "python.jediEnabled": false
}
